{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@swanlab-vue/*": ["vue/src/*"]
    },
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "dist", "vue/public"],
  "include": ["vue/src/**/*.js", "vue/src/**/*.vue", "vue/auto-imports.d.ts"]
}
